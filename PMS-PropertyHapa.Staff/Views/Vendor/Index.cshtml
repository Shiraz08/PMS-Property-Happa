<script>
    $(document).ready(function () {
        $("#addVendorBtn").click(function () {
            $("#vendorModal").modal('show');
        });

        // toastr.options = {
        //     "closeButton": false,
        //     "debug": false,
        //     "newestOnTop": false,
        //     "progressBar": true,
        //     "positionClass": "toast-top-right",
        //     "preventDuplicates": false,
        //     "onclick": null,
        //     "showDuration": "300",
        //     "hideDuration": "1000",
        //     "timeOut": "5000",
        //     "extendedTimeOut": "1000",
        //     "showEasing": "swing",
        //     "hideEasing": "linear",
        //     "showMethod": "fadeIn",
        //     "hideMethod": "fadeOut"
        // };


        let currentFormIndex = 0;
        const formSections = $(".form-section");
        const totalForms = formSections.length;
        formSections.hide().first().show();
        $('#prevButton').hide();
        updateActiveNav(currentFormIndex);

        function resetForms() {
            $('form').each(function () {
                this.reset();
            });
            currentFormIndex = 0;
            formSections.hide().first().show();
            toggleButtons();
            updateActiveNav(currentFormIndex);
        }

        function toggleButtons() {
            $('#nextButton').show();
            $('#saveButton').hide();
            if (currentFormIndex === 0) {
                $('#prevButton').hide();
            } else {
                $('#prevButton').show();
            }
            if (currentFormIndex === totalForms - 1) {
                $('#nextButton').hide();
                $('#saveButton').show();
            }
        }

        function updateActiveNav(index) {
            $('.nav-sidebar .nav-link').removeClass('active');
            $('.nav-sidebar .nav-link').eq(index).addClass('active');
        }

        $(".form-section").not(":first").hide();

        $(".form-section").not(":first").hide();

        $('#nextButton').click(function () {
            if (currentFormIndex === 0) {
                // Validate required fields in the first form
                if (!$('#firstName').val() || !$('#lastName').val()) {
                    toastr.error("Please fill out First Name and Last Name fields.")
                    //alert("Please fill out First Name and Last Name fields.");
                    return;
                }
            }

            if (currentFormIndex < totalForms - 1) {
                formSections.eq(currentFormIndex).hide();
                formSections.eq(++currentFormIndex).show();
                toggleButtons();
                updateActiveNav(currentFormIndex);
            }
        });

        $('#prevButton').click(function () {
            if (currentFormIndex > 0) {
                formSections.eq(currentFormIndex).hide();
                formSections.eq(--currentFormIndex).show();
                toggleButtons();
                updateActiveNav(currentFormIndex);
            }
        });

        // $('#saveButton').click(function () {
        //     alert('Save operation would go here.');
        // });

        $('#vendorModal').on('hidden.bs.modal', function () {
            resetForms();
        });

        toggleButtons();


        $.ajax({
            url: '@Url.Action("GetVendorCategories", "Vendor")',
            type: 'GET',
            dataType: 'json',
            success: function (data) {
                var ownerDropdown = document.getElementById('vendorCategories');
                // ownerDropdown.innerHTML = ''; // Clear existing options
                data.forEach(function (item) {
                    var option = new Option(item.Name, item.VendorCategoryId);
                    ownerDropdown.appendChild(option);
                });
            }
        });

       $('#personalInfo').on('submit', function (event) {
            console.log("1234567")
        event.preventDefault();
        const jsonFormData = gatherFormData();
        });

        function gatherFormData() {
            const formData = {
                PersonalInfoId: $('#personalInfoId').val() || 0,
                FirstName: $("#firstName").val(),
                MiddleName: $('#middleName').val(),
                LastName: $('#lastName').val(),
                Company: $('#company').val(),
                JobTitle: $('#jobTitle').val(),
                Notes: $('#notes').val() || null,
                Email1: $('#email1').val(),
                Phone1: $('#phone1').val(),
                Email2: $('#email2').val(),
                Phone2: $('#phone2').val(),
                Street1: $('#street1').val(),
                Street2: $('#street2').val(),
                District: $('#district').val(),
                City: $('#city').val(),
                State: $('#state').val(),
                Country: $('#country').val(),
                AltStreet1: $('#altStreet1').val(),
                AltStreet2: $('#altstreet2').val(),
                AltDistrict: $('#altdistrict').val(),
                AltCity: $('#altcity').val(),
                AltState: $('#altstate').val(),
                AltCountry: $('#altcountry').val(),
                Service: $('#service').val(),
                VendorCategories: $('#vendorCategories').val(),
                InsuranceCompany: $('#insuranceCompany').val(),
                InsurancePolicyNumber: $('#insurancePolicyNumber').val(),
                PropertyName: $('#propertyName').val(),
                PropertyLocation: $('#propertyLocation').val(),
                TaxID: $('#taxID').val(),
                TaxAmount: $('#taxAmount').val(),
                PaymentMethod: $('#paymentMethod').val(),
                PaymentAmount: $('#paymentAmount').val(),
                
            };
            handleFileUpload(formData);
            return formData;
        }

        function handleFileUpload(formData) {
            const fileInput = $('#PictureUrl')[0];
            if (fileInput.files.length > 0) {
                const reader = new FileReader();
                reader.onload = function (e) {
                    formData.PictureUrl = e.target.result;
                    console.log("43556")
                    submitFormData(formData);
                };
                reader.readAsDataURL(fileInput.files[0]);
            } else {
                submitFormData(formData);
            }
        }

        function submitFormData(jsonFormData) {
            $.ajax({
                url: '@Url.Action("SaveVendor", "Vendor")',
                type: 'POST',
                data: JSON.stringify(jsonFormData),
                contentType: 'application/json',
                beforeSend: function () {
                    console.log("123234")
                    $('#saveButton').prop('disabled', true).text('Saving...');
                },
                success: function (response) {
                    $("#vendorModal").modal('hide');
                    location.reload();
                },
                error: function (xhr, status, error) {
                    console.error('Error:', error);
                    alert('Failed to save data');
                },
                complete: function () {
                    $('#saveButton').prop('disabled', false).text('Save');
                }
            });
        }

      
        
        $('#PictureUrl').on('change', function (event) {
            const fileInput = $('#PictureUrl')[0];
            if (fileInput.files.length > 0) {                                                           
                const reader = new FileReader();
                reader.onload = function (e) {
                    $('#imagePreview').attr('src', e.target.result);
                    $('#imagePreview').show();
                };
                reader.readAsDataURL(fileInput.files[0]);
                console.log("IMAGE");
            }
        });
    });

</script>

